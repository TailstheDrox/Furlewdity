using UnityEngine;
using System.Collections;

public class BaseCharacterSpecies {
	//Species Dependant
	private string characterSpecies;
	private string characterSpeciesDescription;
	private int level;
	private int lust;
	private int height;
	private string cockType;
	private int cockLength;
	private int cockDiameter;
	private int knotDiameter;
	private string ballSize;

	public string CharacterSpecies{get;set;}
	public string CharacterSpeciesDescription{get;set;}
	public int Level{get;set;}
	public int Lust{get;set;}
	public int Height{get;set;}
	public string CockType{get;set;}
	public int CockLength{get;set;}
	public int CockDiameter{get;set;}
	public int KnotDiameter{get;set;}
	public string BallSize{get;set;}
}

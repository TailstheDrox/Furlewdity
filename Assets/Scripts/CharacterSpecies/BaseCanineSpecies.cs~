using UnityEngine;
using System.Collections;

public class BaseCanineSpecies : BaseCharacterSpecies {

	public BaseCanineSpecies(){
		CharacterSpecies = "Canine";
		CharacterSpeciesDescription = "This is the anthropermorhised version of the canid family. More easily excitable than other species they are more adapted than humans.";
		Level = 1;
		Lust = 0;
		Height = 72;
		CockType = "Canine";
		CockLength = 12;
		CockDiameter = 3;
		KnotDiameter = 4;
		BallSize = "Soft Ball";
	}
}
